  },
  "devDependencies": {
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "vite": "^4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run format"
    }
  },
  "browserslist": {
